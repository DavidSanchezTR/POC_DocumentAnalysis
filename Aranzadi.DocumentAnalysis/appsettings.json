{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EnvironmentPrefix": "uksouth-iflx-dev-green-das-",
  "CosmosDatabaseName": "documentAnalysis",
  "KeyVault": {
    "Url": "",
    "CertificateThumbprint": "",
    "ClientAppId": "",
    "ActiveDirectoryTenantId": "",
    "CertificateMode": ""
  },
  "Messaging": {
    "Endpoint": "",
    "Queue": "documentanalysis"
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "LogAnalytics": {
    "WorkspaceId": "",
    "AuthenticationId": "",
    "LogName": "LogsDocumentAnalysis"
  },
  "CheckIfActiveCreditsConsumption": "false",
  "CreditsConsumption": {
    "CreditsConsumptionService": "https://uksouth-iflx-shar-creditsconsumption-dev-app.azurewebsites.net",
    "AzureADCreditsConsumptionScope": "",
    "AzureADCreditsConsumptionTenant": ""
  },
  "AnalysisProvider": {
    "UrlApiJobs": "https://es-casfr-apimgt-test.azure-api.net/analysisapi/v1/analysisJobs",
    "UrlApiDocuments": "https://es-casfr-apimgt-test.azure-api.net/es-casfr-fadocument-test/v1/documents",
    "ApiKey": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.ApplicationInsights", "Serilog.Sinks.AzureAnalytics" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Pooling": {
    "Messaging": {
      "Queue": "pooling"
    },
    "AvailableTime": "0:8:0:0", // days:hours:minutes:seconds
    "RetryTimeInSeconds": 11 // Usado en la formula de calculo del tiempo de retraso de cada reintento
  }
}
